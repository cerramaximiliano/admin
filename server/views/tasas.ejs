      <h2>Tasa de interés en Base de Datos</h2>
      <div class="table-responsive">
        <table class="table table-striped table-sm">
          <thead>
            <tr>
              <th scope="col">Fecha</th>
              <th scope="col">Tasa Pasiva BCRA</th>
              <th scope="col">Tasa Pasiva BNA</th>
              <th scope="col">Tasa Activa BNA</th>
              <th scope="col">Tasa Activa CNAT</th>
            </tr>
          </thead>
          <tbody>

            <% for (var i = 0; i < data.length ; i++) { %>
              <tr>
                <% if (data[i].fecha === undefined || data[i].fecha === null) {%>
                  <td style="    color: red; font-weight: bold;"> N/A </td>
                <% } else { %>
                <td><%= (new Date(data[i].fecha).getUTCDate()).toString() + '/' + (new Date(data[i].fecha).getMonth() + 1).toString() + '/' + (new Date(data[i].fecha).getFullYear()).toString()  %></td>
                <% } %>

                <% if (data[i].tasaPasivaBCRA === undefined || data[i].tasaPasivaBCRA === null) {%>
                  <td style="    color: red; font-weight: bold;"> N/A </td>
                <% } else { %>
                  <td><%= data[i].tasaPasivaBCRA %></td>
                <% } %>

                <% if (data[i].tasaPasivaBNA === undefined || data[i].tasaPasivaBNA === null) {%>
                  <td style="    color: red; font-weight: bold;"> N/A </td>
                <% } else { %>
                  <td><%= data[i].tasaPasivaBNA  %></td>
                <% } %>

                <% if (data[i].tasaActivaBNA === undefined || data[i].tasaActivaBNA === null) {%>
                  <td style="    color: red; font-weight: bold;"> N/A </td>
                <% } else { %>
                  <td><%= data[i].tasaActivaBNA  %></td>
                <% } %>

                <% if (data[i].tasaActivaCNAT2658 === undefined || data[i].tasaActivaCNAT2658 === null) {%>
                  <td style="    color: red; font-weight: bold;"> N/A </td>
                <% } else { %>
                  <td><%= data[i].tasaActivaCNAT2658  %></td>
                <% } %>



              </tr>
            <% }; %>
          </tbody>
        </table>
      </div>

      <div class="row d-flex justify-content-between">
        <div class="card text-bg-light mb-3" style="max-width: 18rem;">
          <div class="card-header">Tasa Pasiva BCRA</div>
          <div class="card-body">
            <h6>Info</h6>
            <div class="d-flex justify-content-around">
              <div>
